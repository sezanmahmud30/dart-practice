  class Father{
    BabarTaka(){

      print("Total Amount = 80000000 Taka");
    }
  }
  class Son extends Father{

  //Overriding
    BabarTaka(){
      print("Total Amount = 50000000 Taka");
    }
  }
  void main(){
    var SonObj = Son();
    SonObj.BabarTaka();

    var FatherObj = Father();
    FatherObj.BabarTaka();
  }
